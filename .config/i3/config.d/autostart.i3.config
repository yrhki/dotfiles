
exec --no-startup-id /usr/lib/xdg-desktop-portal-kde
exec --no-startup-id /usr/lib/xdg-desktop-portal

# Notification service
exec --no-startup-id /usr/bin/dunst

# KDE Connect
exec --no-startup-id /usr/lib/kdeconnectd

# xss-lock grabs a logind suspend inhibit lock and will use i3lock to lock the
# screen before suspend. Use loginctl lock-session to lock your screen.
exec --no-startup-id xss-lock --transfer-sleep-lock -- i3lock --nofork

# start compositor
exec --no-startup-id picom -b --blur-background

# cosair keyboard light
#exec --no-startup-id ckb-next -b

# keyboard layout
exec --no-startup-id setxkbmap -layout yrski/programming
